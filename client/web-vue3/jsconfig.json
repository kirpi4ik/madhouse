{
	"compilerOptions": {
		"target": "es6",
		"module": "esnext",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"src/*": ["src/*"],
			"app/*": ["*"],
			"components/*": ["src/components/*"],
			"layouts/*": ["src/layouts/*"],
			"pages/*": ["src/pages/*"],
			"assets/*": ["src/assets/*"],
			"boot/*": ["src/boot/*"],
			"vue$": ["node_modules/vue/dist/vue.runtime.esm-bundler.js"]
		}
	},
	"lib": ["esnext", "dom", "dom.iterable", "scripthost"],
	"exclude": ["dist", ".quasar", "node_modules"]
}
